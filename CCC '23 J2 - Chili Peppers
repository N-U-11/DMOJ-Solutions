#include <iostream>

using namespace std;

int main() {
    int N;
    cin >> N;
    int C = 0;
    for (int i = 0; i < N; i++){
        string R;
        cin >> R;
        if (R == "Poblano"){
            C += 1500;
        }
        else if (R == "Mirasol"){
            C += 6000;
        }
        else if (R == "Serrano"){
            C += 15500;
        }
        else if (R == "Cayenne"){
            C += 40000;
        }
        else if (R == "Thai"){
            C += 75000;
        }
        else if (R == "Habanero"){
            C += 125000;
        }
        else{
            C += 0;
        }
    }
    cout << C;
    return 0;
}
