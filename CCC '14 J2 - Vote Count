#include <iostream>
#include <string>

using namespace std;

int main() {
    int T;
    cin >> T;
    string C;
    cin >> C;
    int A = 0;
    int B = 0;
    for (int M = 0; M < T; M++){
        if (C[M] == 'A'){
            A += 1;
        }
        else if (C[M] == 'B'){
            B += 1;
        }
        else{
            ;
        }
    }
            if (A > B){
                cout << "A";
            }
            else if (B > A){
                cout << "B";
            }
            else if (B == A){
                cout << "Tie";
            }

    return 0;
}
