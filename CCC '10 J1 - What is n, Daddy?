#include <iostream>

using namespace std;

int main() {
    int N;
    cin >> N;
    if (N < 1 or N > 10){
        abort();
    }
    if (N == 10){
        cout << "1";
    }
    if (N == 9){
        cout << "1";
    }
    if (N == 8){
        cout << "2";
    }
    if (N == 7){
        cout << "2";
    }
    if (N == 6){
        cout << "3";
    }
    if (N == 5){
        cout << "3";
    }
    if (N == 4){
        cout << "3";
    }
    if (N == 3){
        cout << "2";
    }
    if (N == 2){
        cout << "2";
    }
    if (N == 1){
        cout << "1";
    }
    return 0;
}
